          <body class="body" id="body"><noscript><iframe src="https://data.letslucky.com/ns.html?id=GTM-WL4PSTF" height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript><script src="/svg.js?v="></script><script type="text/javascript">
          'use strict';

          const LOADING_ELEMENT_ID = 'x-loading-id';
          const LOADING_HTML_MARKUP = '<div class="loading"><div class="loading__spinner-border" /></div>';

          let isLoading = false;
          let hasClickOccurred = false;
          let clickElement = null;

          const getOrCreateLoadingElement = () => {
            const loadingElement = document.getElementById(LOADING_ELEMENT_ID);
            if (loadingElement) {
              return loadingElement;
            }
            const newLoadingElement = document.createElement('div');
            newLoadingElement.setAttribute('id', LOADING_ELEMENT_ID);
            document.body.appendChild(newLoadingElement);
            return newLoadingElement;
          }

          const showLoading = () => {
          const loadingElement = getOrCreateLoadingElement();
          loadingElement.innerHTML = LOADING_HTML_MARKUP;
          isLoading = true;
          }

          const hideLoading = () => {
            if (isLoading) {
              const loadingElement = getOrCreateLoadingElement();
              loadingElement.remove();
              isLoading = false;
            }
          }

          const handleClickDuringHydration = (event) => {
            event.stopPropagation();
            event.preventDefault();

            if (isLoading) {
              return;
            }

            showLoading();

            clickElement = event.target;

            hasClickOccurred = true;
          }

          document.addEventListener('click', handleClickDuringHydration);

          window.__HYDRATION_DONE_CALLBACK__ = () => {
            document.removeEventListener('click', handleClickDuringHydration);
            hideLoading();

            if (hasClickOccurred) {
              clickElement.click();
            }

            delete window.__HYDRATION_DONE_CALLBACK__;
          }
        </script>
